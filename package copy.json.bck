{
  "name": "hardhat-project",
  "description": "Hardhat framework for new projects",
  "scripts": {
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "build": "hardhat clean && hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage",
    "deploy:testnet": "hardhat deploy --network rinkeby",
    "deploy:fuji": "hardhat deploy --network fuji",
    "prettier": "prettier --write '**/*.{json,ts,sol,md}'",
    "prettier:solidity": "prettier --write contracts/**/*.sol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinix-finance/protocol.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infinix-finance/protocol.git/issues"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run prettier:solidity"
    }
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.4",
    "hardhat": "^2.6.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.11",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.2.0",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@api3/airnode-protocol-v1": "^0.5.1",
    "@eth-optimism/smock": "^1.1.10",
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@opengsn/gsn": "^2.1.0",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/hardhat": "^2.3.0",
    "dotenv": "^16.0.0",
    "prb-math": "1.0.3",
    "solhint": "^3.3.6",
    "solidity-bytes-utils": "0.1.2",
    "ts-generator": "^0.1.1"
  }
}
